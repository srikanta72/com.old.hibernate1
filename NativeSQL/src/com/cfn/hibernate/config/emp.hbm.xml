<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	<hibernate-mapping>
	<class name="com.cfn.hibernate.model.Employee" table="emp">
	<id name="empNumber" column="empno">
	<generator class="assigned"></generator>
	</id>
	<property name="empName" column="ename"  length="10"/>
	<property name="empSalary" column="sal"/>
	<property name="deptNumber" column="deptno"/>
	</class>
	<query name="q1">from Employee e</query>
	<sql-query name="q2">insert into emp (empno,ename,sal,deptno) values (?,?,?,?)</sql-query>
	</hibernate-mapping>